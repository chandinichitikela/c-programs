#include <stdio.h>

int main(){
    int n,i;
    long int array[20];
    
    printf("enter the number range:\n");
    scanf("%d",&n);
    array[0]=0;
    array[1]=1;
    printf("\n FIBONACCI SERIES:\n");
    for(i=2;i<=n+1;i++){
        array[i]=array[i-1]+array[i-2];
        printf("%ld",array[i]);
    }
}
